clear all;
close all;

F0=2; %rondomly decided cos frecuency

t=1:0.01:2*pi; %define time
%plot 5 instances of the given function
figure()
for i =1:5
   X= normrnd(0,1) %gaussian distribution (0,1)
   FI= 2*pi*rand(1) %uniform distribution (
   Yt=X*cos(2*pi*F0*t+FI);
   
   plot(t,Yt)
   hold on;
end